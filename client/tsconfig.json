{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",              
    "paths": {
      "@/*": [
        "./*"
      ],
      "@env": [
        "types/env.d.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "scripts/reset-project.js",
    "types/env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
